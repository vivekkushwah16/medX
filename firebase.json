{
  "hosting": {
    "site": "cipla-impact",
    "public": "build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/accounts",
        "function": "accounts"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
