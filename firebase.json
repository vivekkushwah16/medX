{
  "hosting": {
    "site": "cipla-impact",
    "public": "build",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "/accounts",
        "function": "accounts"
      },
      {
        "source": "/accountsTest",
        "function": "accountsTest"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
